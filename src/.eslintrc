{
  "parser": "babel-eslint",
  "env": {  //enviroment - окружающая среда
    "es6": true,
    "browser": true,
    "node": true
  },
  "rules": {
    "semi": "off",
    "require-jsdoc": "off",
    "indent": ["error", 4],
    "operator-linebreak": "off"
  },
  "extends": [
    "eslint:recommended",
    "google"
  ]
}